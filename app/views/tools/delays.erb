      @xml.Column 0, {'ss:Width' => 60}
      @xml.Column 1, {'ss:Width' => 150}
      @xml.Column 2, {'ss:Width' => 280}
      @xml.Column 3, {'ss:Width' => 60}
      @xml.Column 4, {'ss:Width' => 75}
