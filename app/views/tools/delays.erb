          @xml.Data "Milestone", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'TitleBlue' do
          @xml.Data "Planned date", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'TitleBlue' do
          @xml.Data "Current date", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'TitleBlue' do
          @xml.Data "Delay", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'TitleBlue' do
          @xml.Data "Reason 1", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'TitleBlue' do
          @xml.Data "Reason 2", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'TitleBlue' do
          @xml.Data "Reason 3", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'TitleBlue' do
          @xml.Data "Other", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
      end

      # Rows
      for delay in @delays
        @xml.Row do
          @xml.Cell 'ss:StyleID' => 'borderleft' do
            @xml.Data delay.get_dws, 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
          end
          @xml.Cell do
            @xml.Data delay.get_parent, 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
          end
          @xml.Cell 'ss:StyleID' => 'Pink' do
            @xml.Data delay.project.name, 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
          end
          @xml.Cell do
            @xml.Data delay.get_milestone_name, 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
          end
          @xml.Cell do
            @xml.Data delay.planned_date, 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
          end
          @xml.Cell do
            @xml.Data delay.current_date, 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
          end
          @xml.Cell 'ss:StyleID' => 'Blue' do
            @xml.Data delay.delay_days, 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
          end
          @xml.Cell do
            @xml.Data delay.get_reason_one, 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
          end
          @xml.Cell do
            @xml.Data delay.get_reason_two, 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
          end
          @xml.Cell do
            @xml.Data delay.get_reason_three, 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
          end
          @xml.Cell do
            @xml.Data delay.reason_other, 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
          end
          @xml.Cell 'ss:StyleID' => 'borderleft' do
            @xml.Data "", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
          end
        end #End row
      end #End for

      # Last row
      @xml.Row do
        @xml.Cell 'ss:StyleID' => 'bordertop' do
          @xml.Data "", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'bordertop' do
          @xml.Data "", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'bordertop' do
          @xml.Data "", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'bordertop' do
          @xml.Data "", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'bordertop' do
          @xml.Data "", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'bordertop' do
          @xml.Data "", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'bordertop' do
          @xml.Data "", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'bordertop' do
          @xml.Data "", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'bordertop' do
          @xml.Data "", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'bordertop' do
          @xml.Data "", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
        @xml.Cell 'ss:StyleID' => 'bordertop' do
          @xml.Data "", 'ss:Type' => 'String'; @xml.NamedCell 'ss:Name' => 'FilterDatabase'
        end
      end

    end #End xml.table
  end #End xml.worksheet

end 

%>